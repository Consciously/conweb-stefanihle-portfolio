{
	"name": "gatsby-starter-hello-world",
	"private": true,
	"description": "A simplified bare-bones starter for Gatsby",
	"version": "0.1.0",
	"license": "0BSD",
	"scripts": {
		"//": "⁉️ Hey! This might look confusing but allow me to explain. The command we want to run is called gatsby build. But because we want to use es modules with gatsby, we use a package called esm. One way to require it is to set the NODE_OPTIONS environmental variable to -r esm. Finally to make this work for windows users, we use the cross-env package. Hopefully once Node es modules are stable, we can bring this back to simple gatsby build",
		"build": "gatsby build",
		"develop": "gatsby develop",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"start": "npm run develop",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
	},
	"dependencies": {
		"babel-plugin-styled-components": "^1.11.1",
		"bootstrap": "^4.5.3",
		"dotenv": "^8.2.0",
		"gatsby": "^2.24.85",
		"gatsby-background-image": "^1.1.2",
		"gatsby-image": "^2.4.21",
		"gatsby-plugin-react-helmet": "^3.3.14",
		"gatsby-plugin-remote-images": "^2.2.0",
		"gatsby-plugin-sass": "^2.3.21",
		"gatsby-plugin-sharp": "^2.6.43",
		"gatsby-plugin-styled-components": "^3.3.14",
		"gatsby-source-filesystem": "^2.3.36",
		"gatsby-source-gravatar": "^1.0.0",
		"gatsby-source-sanity": "^6.0.4",
		"gatsby-transformer-sharp": "^2.5.20",
		"node-sass": "^4.14.1",
		"react": "^16.12.0",
		"react-bootstrap": "^1.4.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^6.1.0",
		"react-rotating-text": "^1.4.1",
		"styled-components": "^5.2.1"
	},
	"devDependencies": {
		"cross-env": "^7.0.2",
		"esm": "^3.2.25",
		"gatsby-plugin-html-attributes": "^1.0.5",
		"prettier": "2.1.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Consciously/conweb-stefanihle-portfolio"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	}
}
